#include<stdio.h>
#include<stdlib.h>
void main()
{
         char ch;
         printf("Introduzca una nota:  ");
         scanf("%", &ch);
         swith(ch) 
         {
         case 'A':
                  puts("Exelente.") 
                  break;
         case 'B' puts("Buena.");
                  break;
         case 'C' :
                  puts("Regular.");
                  break;
         case 'D' :
                  puts("Suficiente.");
                  break;
         case 'F' :
                  puts("No suficiente.");
                  break;
         default:   
                  puts("Error");
                  }
                  
             system("pause")
             
}
